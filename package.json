{
    "name": "vue-template",
    "description": "A template with Nuxt3 built on Vue3 with focus on performance and best practices.",
    "licenses": [
        {
            "type": "MIT",
            "url": "https://github.com/krudi/vue-template/LICENSE"
        }
    ],
    "version": "0.1.0",
    "authors": [
        {
            "name": "krudi",
            "email": "patryk.kudlik@gmail.com",
            "url": "https://github.com/krudi/"
        }
    ],
    "private": false,
    "homepage": "https://github.com/krudi/vue-template#readme",
    "bugs": {
        "url": "https://github.com/krudi/vue-template/issues"
    },
    "keywords": [
        "template"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/krudi/vue-template.git"
    },
    "engines": {
        "node": ">= 23.11.0",
        "npm": ">= 10.8.1"
    },
    "type": "module",
    "scripts": {
        "dev": "nuxt dev",
        "build": "nuxt build",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "start": "node .output/server/index.mjs",
        "lint:js": "eslint -c ./eslint.config.ts",
        "lint:js:fix": "eslint -c ./eslint.config.ts --fix",
        "lint:css": "stylelint --c ./stylelint.config.ts **/*.css",
        "lint:css:fix": "stylelint -c ./stylelint.config.ts --fix **/*.css",
        "lint:files": "prettier --check \"**/*.{js,ts,vue,css,html,yaml,yml,json,json5,graphql,gql}\"",
        "lint:files:fix": "prettier --write \"**/*.{js,ts,vue,css,html,yaml,yml,json,json5,graphql,gql}\"",
        "lint:full": "npm run lint:js && npm run lint:css && npm run lint:files",
        "lint:full:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:files:fix"
    },
    "devDependencies": {
        "@nuxt/devtools": "^2.4.1",
        "@nuxt/eslint": "^1.4.1",
        "@nuxt/eslint-config": "^1.3.1",
        "@nuxt/types": "^2.18.1",
        "@types/node": "^22.15.23",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.27.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-vue": "^10.1.0",
        "install": "^0.13.0",
        "jiti": "^2.4.2",
        "npm": "^11.4.1",
        "nuxt": "^3.17.4",
        "postcss": "^8.5.4",
        "postcss-import": "^16.1.0",
        "postcss-load-config": "^6.0.1",
        "postcss-nested": "^7.0.2",
        "prettier": "^3.5.3",
        "stylelint": "^16.19.1",
        "stylelint-config-standard": "^37.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.33.0",
        "vue-tsc": "^2.2.10"
    }
}
